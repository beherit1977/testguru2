<h1>Test: <%= @test.title %></h1>

<p>Difficulty: <%= @test.level %></p>
<p>Publication date: <%= @test.created_at.strftime('%Y-%m-%d %H:%M') %></p>
<p><%= link_to 'New Question', new_admin_test_question_path(@test) %></p>

<table>
  <tr>
    <th>Question</th>
    <th colspan="3">Actions</th>
  </tr>
  <%#= render @test.questions %>
  <%= render partial: 'admin/questions/question', collection: @test.questions %>
</table>

<p><%= link_to 'Edit', edit_admin_test_path(@test) %></p>
<p><%= link_to 'Back', admin_tests_path %></p>
