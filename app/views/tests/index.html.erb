<p id="notice"><%= notice %></p>

<h1>Tests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= test.questions.count %></td>
        <td><%= link_to 'Show', test_path(test) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
